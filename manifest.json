{
  "manifest_version": 3,
  "name": "Vaulter",
  "version": "1.0",
  "description": "Get login details from g-sheets",
  "permissions": ["identity", "storage", "activeTab", "scripting", "webRequest"],
  "host_permissions": ["https://*/*", "http://*/*"],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "./assets/icon.png",
      "48": "./assets/icon.png",
      "128": "./assets/icon.png"
    }
  },
  "oauth2": {
    "client_id": "224544091554-4i5a8hkdcqg8jusu1b8hn86uhq4csn8j.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.readonly"
    ]
  }
}
